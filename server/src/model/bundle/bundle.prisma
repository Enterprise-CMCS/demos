model Bundle {
  id           String @id @default(uuid()) @db.Uuid
  bundleTypeId String @map("bundle_type_id")

  bundleType    BundleType     @relation(fields: [bundleTypeId], references: [id])
  demonstration Demonstration?
  document      Document[]

  @@unique([id, bundleTypeId])
  @@map("bundle")
}
