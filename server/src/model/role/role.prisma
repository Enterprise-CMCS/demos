// Flags: STATIC_CONSTRAINT
model Role {
  id           String          
  grantLevelId String @map("grant_level_id")

  grantLevel GrantLevel @relation(fields: [grantLevelId], references: [id])
  
  events          Event[]
  rolePermissions RolePermission[]

  @@id([id])
  @@unique([id, grantLevelId])
  @@map("role")
}
