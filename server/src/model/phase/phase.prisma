// Flags: STATIC_CONSTRAINT
model Phase {
  id          String
  phaseNumber Int    @map("phase_number")

  bundlePhaseStatus BundlePhaseStatus[]
  demonstration     Demonstration[]
  modification      Modification[]
  bundlePhaseDate   BundlePhaseDate[]

  @@id([id])
  @@unique([phaseNumber])
  @@map("phase")
}
