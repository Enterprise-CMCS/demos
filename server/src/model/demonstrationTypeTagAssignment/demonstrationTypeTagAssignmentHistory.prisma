model DemonstrationTypeTagAssignmentHistory {
  revisionId      Int          @id @default(autoincrement()) @map("revision_id")
  revisionType    RevisionType @map("revision_type")
  modifiedAt      DateTime     @default(now()) @map("modified_at") @db.Timestamptz()
  demonstrationId String       @map("demonstration_id") @db.Uuid
  tagId           String       @map("tag_id")
  tagTypeId       String       @map("tag_type_id")
  effectiveDate   DateTime     @map("effective_date") @db.Timestamptz()
  expirationDate  DateTime     @map("expiration_date") @db.Timestamptz()
  createdAt       DateTime     @map("created_at") @db.Timestamptz()
  updatedAt       DateTime     @map("updated_at") @db.Timestamptz()

  @@map("demonstration_type_tag_assignment_history")
}
