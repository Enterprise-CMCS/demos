{
  "name": "demos-server",
  "version": "0.1.0",
  "type": "module",
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/aws-lambda": "^4.0.1",
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/client-secrets-manager": "^3.950.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@date-fns/tz": "^1.4.1",
    "@prisma/client": "^6.19.1",
    "date-fns": "^4.1.0",
    "graphql-scalars": "^1.25.0",
    "graphql-tools": "^9.0.25",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.0",
    "pino": "^10.1.0",
    "prisma-extension-random": "^0.2.2",
    "react": "^19.2.3"
  },
  "overrides": {
    "qs": "6.14.1"
  },
  "scripts": {
    "clean": "rm -rf dist; npm ci",
    "build": "prisma generate && tsc && esbuild dist/server.js --bundle --platform=node --outfile=build/server.cjs --minify --external:@yaacovcr/transform",
    "build:ci": "prisma generate && tsc && esbuild dist/server.js --bundle --platform=node --outfile=build/server.cjs --minify --external:@yaacovcr/transform && mkdir -p build/node_modules/.prisma/client/ && cp node_modules/.prisma/client/libquery_engine-rhel-openssl-3.0.x.so.node build/node_modules/.prisma/client/",
    "depcheck": "depcheck",
    "server": "node ./dist/server.js",
    "dev": "tsx watch --inspect=9229 --clear-screen=false src/local-server.ts",
    "watch": "concurrently \"tsx watch --inspect=9229 --clear-screen=false src/local-server.ts\" \"prisma generate --watch\"",
    "prisma": "prisma generate --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prisma:format": "prisma format",
    "seed:reset": "npm run migrate:reset && npm run dbrefresh && npm run seed",
    "seed": "tsx --inspect ./src/seeder.ts",
    "migrate:empty": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset --force",
    "migrate:deploy": "prisma migrate deploy && npm run dbrefresh",
    "lint:debug": "node --inspect-brk $(which eslint) .",
    "test": "vitest run",
    "test:verbose": "vitest run --reporter=verbose",
    "coverage": "vitest run --coverage",
    "coverage:ci": "vitest run --no-color --coverage --coverage.reporter=lcov --reporter=default --reporter=vitest-sonar-reporter --outputFile=coverage/test-results.xml",
    "dbrefresh": "tsx --inspect ./src/refreshDbObjects.ts"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/client-secrets-manager": "^3.950.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^9.9.0",
    "@graphql-tools/merge": "^9.1.6",
    "@parcel/watcher": "^2.5.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "depcheck": "^1.4.7",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.2",
    "eslint-utils": "^3.0.0",
    "graphql": "^16.12.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.7.4",
    "prisma": "^6.19.1",
    "ts-jest": "^29.4.6",
    "tsc-watch": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vitest": "^3.2.4",
    "vitest-sonar-reporter": "^2.0.4"
  }
}
