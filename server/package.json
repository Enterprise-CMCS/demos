{
  "name": "demos-server",
  "version": "0.1.0",
  "type": "module",
  "prisma": {
    "schema": "./src/model"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@as-integrations/aws-lambda": "^3.1.0",
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/client-secrets-manager": "^3.840.0",
    "@aws-sdk/s3-request-presigner": "^3.859.0",
    "@date-fns/tz": "^1.4.1",
    "@prisma/client": "^6.17.0",
    "date-fns": "^4.1.0",
    "graphql-scalars": "^1.24.2",
    "graphql-tools": "^9.0.18",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "prisma-extension-random": "^0.2.2",
    "react": "^19.2.0"
  },
  "scripts": {
    "clean": "rm -rf dist; npm ci",
    "build": "prisma generate && tsc && esbuild dist/server.js --bundle --platform=node --outfile=build/server.cjs --minify",
    "build:ci": "prisma generate && tsc && esbuild dist/server.js --bundle --platform=node --outfile=build/server.cjs --minify && mkdir -p build/node_modules/.prisma/client/ && cp node_modules/.prisma/client/libquery_engine-rhel-openssl-3.0.x.so.node build/node_modules/.prisma/client/",
    "depcheck": "depcheck",
    "server": "node ./dist/server.js",
    "dev": "tsx watch --inspect=9229 --clear-screen=false src/local-server.ts",
    "watch": "concurrently \"tsx watch --inspect=9229 --clear-screen=false src/local-server.ts\" \"prisma generate --watch\"",
    "prisma": "prisma generate --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prisma:format": "prisma format",
    "seed:reset": "npm run migrate:reset && npm run dbrefresh && npm run seed",
    "seed": "tsx --inspect ./src/seeder.ts",
    "migrate:empty": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset --force",
    "migrate:deploy": "prisma migrate deploy && npm run dbrefresh",
    "lint:debug": "node --inspect-brk $(which eslint) .",
    "test": "vitest run",
    "test:verbose": "vitest run --reporter=verbose",
    "coverage": "vitest run --coverage",
    "coverage:ci": "vitest run --no-color --coverage --coverage.reporter=lcov --reporter=default --reporter=vitest-sonar-reporter --outputFile=coverage/test-results.xml",
    "dbrefresh": "tsx --inspect ./src/refreshDbObjects.ts"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/client-secrets-manager": "^3.840.0",
    "@aws-sdk/s3-request-presigner": "^3.859.0",
    "@eslint/js": "^9.27.0",
    "@faker-js/faker": "^9.8.0",
    "@graphql-tools/merge": "^9.0.24",
    "@parcel/watcher": "^2.5.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "artillery": "^2.0.23",
    "concurrently": "^9.1.2",
    "depcheck": "^1.4.7",
    "esbuild": "^0.25.5",
    "eslint": "^9.27.0",
    "eslint-utils": "^3.0.0",
    "graphql": "^16.11.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.15.0",
    "ts-jest": "^29.3.3",
    "tsc-watch": "^7.1.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.2.4",
    "vitest-sonar-reporter": "^2.0.4"
  }
}
