{
  "name": "demos-server",
  "version": "0.1.0",
  "type": "module",
  "prisma": {
    "schema": "./src/model"
  },
  "dependencies": {
    "@graphql-tools/merge": "^9.0.24",
    "@prisma/client": "^6.7.0",
    "depcheck": "^1.4.7",
    "graphql-scalars": "^1.24.2",
    "graphql-tools": "^9.0.18",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "prettier": "3.5.3",
    "prisma-extension-random": "^0.2.2",
    "react": "^19.1.0"
  },
  "scripts": {
    "build": "npm install; rm -rf dist; npx tsc",
    "dev": "npm run build && node ./dist/index.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "watch": "concurrently \"tsx watch --clear-screen=false src/index.ts\" \"npx prisma generate --watch\"",
    "seed": "npx prisma db push --force-reset && tsx --inspect ./src/seeder.ts"
  },
  "devDependencies": {
    "@apollo/server": "^4.12.1",
    "@eslint/js": "^9.27.0",
    "@faker-js/faker": "^9.8.0",
    "@parcel/watcher": "^2.5.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "artillery": "^2.0.23",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-utils": "^3.0.0",
    "graphql": "^16.11.0",
    "jest": "^29.7.0",
    "prisma": "^6.6.0",
    "ts-jest": "^29.3.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  }
}
