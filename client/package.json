{
  "name": "demos-client",
  "description": "The client application for Demonstration Evaluation & Management Oversight System (DEMOS).",
  "version": "0.1.0",
  "type": "module",
  "devDependencies": {
    "@apollo/client": "^3.14.0",
    "@axe-core/cli": "^4.11.0",
    "@date-fns/tz": "^1.4.1",
    "@date-fns/utc": "^2.1.1",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^9.9.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^4.0.15",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-idle-timer": "^5.7.2",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^6.4.1",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15",
    "vitest-sonar-reporter": "^3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 edge version",
      "last 2 safari version"
    ]
  },
  "scripts": {
    "ally": "echo 'Running accessibility check...' && echo 'Make sure dev server is running (npm run dev)' && axe http://localhost:3000 --save accessibility-report.json --verbose --timer --tags wcag2a,wcag2aa,wcag21aa && echo 'Accessibility report saved to accessibility-report.json'",
    "ally:ci": "axe http://localhost:3000 --save accessibility-report.json --exit --no-reporter",
    "analyze": "vite-bundle-visualizer --mode=build",
    "build": "npm run tsc && vite build",
    "build:ci": "vite build",
    "coverage": "vitest --coverage",
    "coverage:ci": "vitest run --no-color --coverage --coverage.reporter=lcov --reporter=default --reporter=vitest-sonar-reporter --outputFile=coverage/test-results.xml",
    "depcheck": "depcheck",
    "dev": "vite --host",
    "dev:mocks": "VITE_USE_MOCKS=true npm run dev",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "eslint src; npm run tsc",
    "lint:ci": "eslint src",
    "lint:fix": "eslint src --fix",
    "preview": "vite preview",
    "test": "vitest --mode test",
    "test:once": "vitest --mode test --run --reporter=dot",
    "test:serial": "vitest --mode test --run --no-file-parallelism",
    "test:debug": "vitest --mode test --run --inspect-brk --no-file-parallelism",
    "tsc": "tsc --noEmit --skipLibCheck",
    "tsc:ci": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "@testing-library/user-event": "^14.6.1",
    "date-fns": "^4.1.0",
    "react-oidc-context": "^3.3.0",
    "react-router-dom": "^7.10.1",
    "react-switch": "^7.1.0"
  },
  "optionalDependencies": {
    "@emnapi/core": "^1.7.1",
    "@rollup/rollup-linux-x64-musl": "^4.53.4",
    "lightningcss": "^1.30.2"
  }
}
